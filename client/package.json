    "monday-sdk-js": "^0.1.3",
    "monday-ui-react-core": "1.54.0",
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "concurrently": "^5.2.0",
    "cross-port-killer": "^1.2.1",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "ngrok": "^4.3.1",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8"
  }
